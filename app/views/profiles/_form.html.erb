<%= render('layouts/errors', :object => @profile) %>

<%= form_for [@profile] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <br>
  <%= f.label :gender %>
  <%= f.text_field :gender %>
  <br>
  <%= f.label :avatar %>
  <%= f.file_field :avatar %>
  <p><%= f.submit %></p>
<% end %>
